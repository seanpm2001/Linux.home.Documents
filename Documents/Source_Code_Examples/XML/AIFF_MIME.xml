<!-- Start of script !-->
<!-- This alternative MIME type for SVG Video is AIFF, as not everybody will want to use OGG, and AIFF playback may be needed !-->
<audioMIMEType>
  <!-- The default audio MIME type for SVG video !-->
  <webMIMEType> <!-- For websites and web applications !-->
  <!--
  AIFF formats
  .AIFF
  Supported AIFF formats
  .AIFF
  !-->
  <!-- Not supported: video/mp4, !-->
    <AIFFMIMETypeTrueAudio>audio>AIFF</AIFFMIMETypeTrueAudio>
  <!-- Not supported: application/AIFF !-->
  </webMIMEType>
  <MIMETypeAudio> <!-- For non-web applications !-->
    <isoMIMETypeA>audio/AIFF</isoMIMETypeA>
    </MIMETypeAudio>
</audioMIMEType>
